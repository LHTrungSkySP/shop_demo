{
  "name": "shop-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve master-app --configuration development",
    "start:master-app": "ng serve master-app --configuration production",
    "start:shopping": "ng serve shopping",
    "start:manage": "ng serve manage",
    "start:all": "npm run start && npm run start:shopping && npm run start:manage",
    "start-prod:all": "npm run start:master-app && npm run start:shopping && npm run start:manage",
    "vercel-start": "npm run start:all",
    "build:all": "ng build master-app --configuration development && ng build shopping && ng build manage",
    "build-prod:all": "ng build master-app --configuration production && ng build shopping --configuration production && ng build manage --configuration production",
    "vercel-build": "npm run build-prod:all",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/module-federation": "^18.0.2",
    "@angular/animations": "^17.3.0",
    "@angular/cdk": "^18.0.4",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "primeflex": "^3.3.1",
    "primeicons": "^7.0.0",
    "primeng": "^17.18.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.8",
    "@angular/cli": "^17.3.5",
    "@angular/compiler-cli": "^17.3.0",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ngx-build-plus": "^18.0.0",
    "typescript": "~5.4.2",
    "vercel": "^34.2.7"
  }
}
