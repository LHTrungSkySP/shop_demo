{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/master-app/(.*)",
      "dest": "/master-app/index.html"
    },
    {
      "src": "/shopping/(.*)",
      "dest": "/shopping/index.html"
    },
    {
      "src": "/manage/(.*)",
      "dest": "/manage/index.html"
    }
  ]
}
